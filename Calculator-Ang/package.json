{
  "name": "calculator-ang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "prestart": "node aspnetcore-https",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ci:start-server": "angular-http-server --path ./dist -p 4200",
    "ci:run": "start-server-and-test ci:start-server http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.9",
    "@angular/common": "~12.2.9",
    "@angular/compiler": "~12.2.9",
    "@angular/core": "~12.2.9",
    "@angular/forms": "~12.2.9",
    "@angular/platform-browser": "~12.2.9",
    "@angular/platform-browser-dynamic": "~12.2.9",
    "@angular/router": "~12.2.9",
    "jest-editor-support": "*",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/compiler-cli": "~12.2.9",
    "@cypress/schematic": "^1.5.1",
    "@types/jasmine": "~3.9.1",
    "@types/node": "^16.10.9",
    "angular-http-server": "^1.10.0",
    "cypress": "8.5.0",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-check-updates": "^11.8.5",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.3.5"
  }
}